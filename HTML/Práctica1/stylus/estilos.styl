@import "../css/normalize.css"
body
  background-color: #ff6347
  text-size:16px
h1, table h1
  color: white
  text-align: center
hr
  border-top: 2px dashed #BF351C
  border-color:#BF351C
  color: #ff6347
table
  margin: 0 auto
  background: white

.center-frame
  display: block
  margin: 0 auto
  text-align: center
iframe
  display: inline-block
footer
  border-top: 2px dashed
  border-color: #BF351C;
footer
  & .footer-divider
    display: inline-block
    width: 30%
footer
  & .footer-divider
    & h3
      text-align: center
      color: #BF351C
table
  border: 1px solid
  border-color: #BF351C
